{
  "name": "week3-express-mvc",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "locall:dev": "cross-env NODE_ENV=dev nodemon ./bin/www ",
    "locall:prod": "cross-env NODE_ENV=production nodemon ./bin/www ",
    "server": "cross-env NODE_ENV=production nodemon ./bin/www",
    "swagger:dev": "cross-env NODE_ENV=dev node ./swagger.js",
    "swagger:prod": "cross-env NODE_ENV=production node ./swagger.js",
    "swagger": "cross-env npm run swagger:dev\n npm run swagger:prod"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "debug": "~2.6.9",
    "dotenv": "^16.0.0",
    "express": "~4.16.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.3.2",
    "morgan": "~1.9.1",
    "swagger-autogen": "^2.5.10",
    "swagger-ui-express": "^4.3.0",
    "validator": "^13.7.0"
  }
}
